{
    "type": "object", 
    "properties": {
        "routing-instance": {
            "type": "object", 
            "properties": {
                "display_name": {
                    "required": "optional", 
                    "type": "string", 
                    "description": "Display name user configured string(name) that can be updated any time. Used as openstack name."
                }, 
                "routing_instance_has_pnf": {
                    "required": "system-only", 
                    "type": "boolean", 
                    "description": "Internal service chaining information, should not be modified."
                }, 
                "routing_instance_is_default": {
                    "required": "system-only", 
                    "type": "boolean", 
                    "description": "Internal service chaining information, should not be modified."
                }, 
                "parent_type": {
                    "required": "required", 
                    "type": "string", 
                    "enum": [
                        "virtual-network"
                    ]
                }, 
                "perms2": {
                    "required": "system-only", 
                    "type": "object", 
                    "properties": {
                        "owner": {
                            "required": "true", 
                            "type": "string", 
                            "description": "Owner tenant of the object"
                        }, 
                        "owner_access": {
                            "required": "true", 
                            "type": "number", 
                            "description": "Owner permissions of the object"
                        }, 
                        "global_access": {
                            "required": "optional", 
                            "type": "number", 
                            "description": "Globally(others) shared object and permissions for others of the object"
                        }, 
                        "share": {
                            "items": {
                                "type": "object", 
                                "properties": {
                                    "tenant_access": {
                                        "required": "true", 
                                        "type": "number", 
                                        "description": "Allowed permissions in sharing"
                                    }, 
                                    "tenant": {
                                        "required": "true", 
                                        "type": "string", 
                                        "description": "Name of tenant with whom the object is shared"
                                    }
                                }
                            }, 
                            "required": "optional", 
                            "type": "array", 
                            "description": "Selectively shared object, List of (tenant, permissions)"
                        }
                    }, 
                    "description": "Permissions data for role based access."
                }, 
                "service_chain_information": {
                    "required": "system-only", 
                    "type": "object", 
                    "properties": {
                        "routing_instance": {
                            "type": "string"
                        }, 
                        "source_routing_instance": {
                            "type": "string"
                        }, 
                        "prefix": {
                            "items": {
                                "type": "string"
                            }, 
                            "type": "array"
                        }, 
                        "service_instance": {
                            "type": "string"
                        }, 
                        "service_chain_address": {
                            "type": "string"
                        }
                    }, 
                    "description": "Internal service chaining information, should not be modified."
                }, 
                "id_perms": {
                    "required": "system-only", 
                    "type": "object", 
                    "properties": {
                        "enable": {
                            "required": "true", 
                            "type": "boolean", 
                            "description": "Administratively Enable/Disable this object"
                        }, 
                        "description": {
                            "required": "optional", 
                            "type": "string", 
                            "description": "User provided text"
                        }, 
                        "created": {
                            "required": "system-only", 
                            "type": "string", 
                            "description": "Time when this object was created"
                        }, 
                        "creator": {
                            "required": "system-only", 
                            "type": "string", 
                            "description": "Id of tenant who created this object"
                        }, 
                        "user_visible": {
                            "required": "system-only", 
                            "type": "boolean", 
                            "description": "System created internal objects will have this flag set and will not be visible"
                        }, 
                        "last_modified": {
                            "required": "system-only", 
                            "type": "string", 
                            "description": "Time when this object was created"
                        }, 
                        "permissions": {
                            "required": "optional", 
                            "type": "object", 
                            "properties": {
                                "owner": {
                                    "type": "string"
                                }, 
                                "owner_access": {
                                    "type": "number"
                                }, 
                                "other_access": {
                                    "type": "number"
                                }, 
                                "group": {
                                    "type": "string"
                                }, 
                                "group_access": {
                                    "type": "number"
                                }
                            }, 
                            "description": "No longer used, will be removed"
                        }, 
                        "uuid": {
                            "required": "true", 
                            "type": "object", 
                            "properties": {
                                "uuid_mslong": {
                                    "type": "number"
                                }, 
                                "uuid_lslong": {
                                    "type": "number"
                                }
                            }, 
                            "description": "UUID of the object, system automatically allocates one if not provided"
                        }
                    }, 
                    "description": "System maintained identity, time  and permissions data."
                }, 
                "ipv6_service_chain_information": {
                    "required": "system-only", 
                    "type": "object", 
                    "properties": {
                        "routing_instance": {
                            "type": "string"
                        }, 
                        "source_routing_instance": {
                            "type": "string"
                        }, 
                        "prefix": {
                            "items": {
                                "type": "string"
                            }, 
                            "type": "array"
                        }, 
                        "service_instance": {
                            "type": "string"
                        }, 
                        "service_chain_address": {
                            "type": "string"
                        }
                    }, 
                    "description": "Internal service chaining information, should not be modified."
                }, 
                "static_route_entries": {
                    "required": "system-only", 
                    "type": "object", 
                    "properties": {
                        "route": {
                            "items": {
                                "type": "object", 
                                "properties": {
                                    "route_target": {
                                        "items": {
                                            "type": "string"
                                        }, 
                                        "type": "array"
                                    }, 
                                    "prefix": {
                                        "type": "string"
                                    }, 
                                    "next_hop": {
                                        "type": "string"
                                    }, 
                                    "community": {
                                        "items": {
                                            "type": "string"
                                        }, 
                                        "type": "array"
                                    }
                                }
                            }, 
                            "type": "array"
                        }
                    }, 
                    "description": "Internal service chaining information, should not be modified."
                }, 
                "route_target_refs": {
                    "url": "/route-targets", 
                    "type": "array"
                }, 
                "routing_instance_refs": {
                    "url": "/routing-instances", 
                    "type": "array"
                }, 
                "default_ce_protocol": {
                    "required": "optional", 
                    "type": "object", 
                    "properties": {
                        "bgp": {
                            "type": "object", 
                            "properties": {}
                        }, 
                        "ospf": {
                            "type": "object", 
                            "properties": {
                                "area": {
                                    "type": "number"
                                }
                            }
                        }
                    }, 
                    "description": ""
                }
            }
        }
    }
}