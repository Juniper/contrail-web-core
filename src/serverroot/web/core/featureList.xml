<!--
    read-access  - read-access to which UI roles permitted.
                   If not specified, then read-access is allowed to all UI
                   Roles.
    write-access - write-access to which UI roles permitted
                   If not specified, then write-access is allowed to all UI Roles.
-->
<featureLists>
    <require>
        <define>parseFeatureReq</define>
        <path>../../common/parseFeatureRequire</path>
    </require>
    <item>
        <feature>admin-config</feature>
    </item>
    <item>
        <feature>monitoring</feature>
    </item>
    <item>
        <feature>orchestration</feature>
    </item>
    <item>
        <feature>setting</feature>
    </item>
    <item>
        <feature>proxy</feature>
    </item>
    <item>
        <feature>alarms</feature>
        <read-access>member</read-access>
        <write-access>member</write-access>
    </item>
    <item>
        <feature>discoveryclient</feature>
    </item>
    <item>
        <feature>configschema</feature>
    </item>
</featureLists>
