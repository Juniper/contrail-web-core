<script type="text/x-handlebars-template" id="alerts-template">
    <ul class="item-list">
        {{#each this}}
            <div class="itemdiv commentdiv infobox-blue">
                <div class="body">
                    {{#typeof timeStamp "string"}}
                        <div class="text word-break-all" style="width:75%">
                    {{/typeof}}
                    {{#IfCompare timeStamp null operator="=="}}
                        <div class="text word-break-all">
                    {{/IfCompare}}
                    {{#IfCompare name null operator="=="}}
                         {{msg}}</div>
                    {{else}}
                        {{#IfCompare link null operator="!="}}
                            <span class="hyperlink" onclick=javascript:layoutHandler.setURLHashObj({{#jsonStringify link}}{{/jsonStringify}})>{{name}}</span> , {{type}} , {{msg}}</div>
                        {{else}}
                             {{name}} , {{type}} , {{msg}}</div>
                        {{/IfCompare}}
                    {{/IfCompare}}
                    {{#typeof timeStamp "string"}}
                        <div class="time" style="width:90px">
                            <i class="fa fa-clock-o"></i>
                            <span> {{#getRelativeTime timeStamp}}{{/getRelativeTime}} </span>
                        </div>
                    {{/typeof}}
                </div>
            </div>
            {{else}}
                <p>No Alarms found.</p>
        {{/each}}
        {{#IfCompare this.length 0 operator=">"}}
            <div id="moreAlertsLink" style="cursor:pointer;margin-left:5px"><a>More</a></div>
        {{/IfCompare}}
    </ul>
</script>

